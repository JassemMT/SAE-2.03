<webconf>
    <port>800</port>
    <DocumentRoot>var/www</DocumentRoot>
    <security>
        <order>
        <first>accept</first>
        <last>reject</last>
        </order>
        <default>reject</default>
        <accept>192.168.0.0/24</accept>
        <accept>172.23.96.1/</accept>
        <accept>127.0.0.1</accept>
        <reject>192.168.1.0/24</reject>
    </security>
    <accesslog>/var/log/myweb/access.log</accesslog>
    <errorlog>/var/log/myweb/error.log</errorlog>
</webconf>
